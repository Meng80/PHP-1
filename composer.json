{
    "name": "miw_upm/miw_results",
    "license": "MIT",
    "type": "project",
    "description": "MiW ORM Doctrine - Results",
    "homepage": "https://miw.etsisi.upm.es/",
    "keywords": ["doctrine-orm", "dotenv"],
    "authors": [
        {
            "name": "MiW UPM",
            "homepage": "https://miw.etsisi.upm.es/",
            "email": "miw.etsisi@upm.es",
            "role": "Developer"
        }
        ],
    "autoload": {
        "psr-4": {
            "MiW\\Results\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiW\\Results\\Tests\\": "tests/"
        }
    },
    "require": {
		"php": ">=8.0",
        "ext-mysqli": "*",
        "ext-json": "*",
        "doctrine/orm": "^2.17",
        "vlucas/phpdotenv": "^5.5",
        "symfony/config": "^6.3",
        "symfony/cache": "^6.3",
        "symfony/routing": "^6.3",
        "symfony/yaml": "^6.3",
        "catfan/medoo": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
